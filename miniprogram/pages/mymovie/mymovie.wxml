<!--pages/mymovie/mymovie.wxml-->
<!-- 收藏组件  -->

<van-button icon="star-o" type="primary" bindtap="upload">上传图片</van-button>
<view>
<!-- 图片预览 -->
<image 
wx:for="{{imglist}}" wx:key="index" src="{{item}}"
style="width:200rpx;height:200rpx;"
></image>
</view>
<button style="border:1px solid #afa;width:100%" size="large"
bindtap="submit"
>提交</button>
<van-button type="primary" block bindtap="jumpDetail">查询</van-button>